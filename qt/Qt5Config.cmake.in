@PACKAGE_INIT@

set(_PREFIX_DIR @PACKAGE_PREFIX_DIR@)

set(Qt5_FOUND FALSE)
set(Qt5_DIR)

find_package(Qt5 REQUIRED
    COMPONENTS ${Qt6_FIND_COMPONENTS}
    )

set(_src_dir "")
set(_lib_dir "@PACKAGE_PYNCPP_PYTHON_DIR@/lib/python@PYNCPP_PYTHON_SHORT_VERSION@/site-packages/PySide2/Qt/lib")

foreach(module ${Qt5_FIND_COMPONENTS})
    set(target Qt5::${module})
    set(location "${_lib_dir}/Qt${module}.framework/Versions/5/Qt${module}")
    set_target_properties(${target} PROPERTIES
        IMPORTED_LOCATION "${location}"
        )
endforeach()

check_required_components(Qt5)
